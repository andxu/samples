    "version": "0.2.0",
    "configurations": [
        {
            "type": "java-azds",
            "name": "Debug (Launch)",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "port": 7000
        },
        {
            "type": "java",
            "name": "Debug (Launch)-MyMain<module2>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "mainClass": "MyMain",
            "args": "",
            "projectName": "module2"
        },
        {
            "type": "java",
            "name": "Debug (Attach)",
            "request": "attach",
            "hostName": "localhost",
            "port": "<debug port of remote debuggee>"
        }
    ]
}